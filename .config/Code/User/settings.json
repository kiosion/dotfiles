{
  "eslint.workingDirectories": [
    "./sanity-cms",
    "./react-app"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "svelte"
],
"php.validate.executablePath": "/opt/lampp/bin/php",
  "editor.suggestSelection": "first",
  "editor.fontLigatures": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.fontFamily": "MonoLisa Nerd Font, 'Cascadia Code', 'Fira Code iScript', monospace",
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "vsicons.dontShowNewVersionMessage": true,
  "explorer.confirmDelete": false,
  "git.autofetch": true,
  "explorer.confirmDragAndDrop": false,
  "git.enableCommitSigning": true,
  "security.workspace.trust.untrustedFiles": "open",
  "gitlens.defaultTimeFormat": "H:mm",
  "codetogether.userName": "kio",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true,
    "python": true,
    "scss": true,
    "typescript": true,
    "elixir": true
  },
  "files.associations": {
    "*.css": "css",
  },
  // "vscode_custom_css.imports": [
  //   "file:///home/kio/.vscode/extensions/webrender.synthwave-x-fluoromachine-0.0.12/synthwave-x-fluoromachine.css"
  // ],
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "keyword",
          "storage.modifier",
          "storage.type.class.js",
          "storage.type.js"
        ],
        "settings": {
          "fontStyle": "italic bold"
        }
      },
      {
        "scope": [
          "keyword.control.conditional",
          "keyword.operator"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      }
    ]
  },
  "discord.lowerDetailsEditing": "Line: {current_line} of {total_lines}",
  "discord.lowerDetailsDebugging": "{empty}",
  "editor.mouseWheelZoom": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.smoothScrolling": true,
  "workbench.editor.untitled.hint": "hidden",
  "files.insertFinalNewline": true,
  "explorer.compactFolders": false,
  "tabnine.experimentalAutoImports": true,
  "terminal.integrated.defaultProfile.osx": "fish",
  "rpc.appName": "Visual Studio Code",
  "rpc.showProblems": false,
  "rpc.suppressNotifications": false,
  "editor.tabSize": 2,
  "editor.accessibilitySupport": "off",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.renderWhitespace": "all",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": true,
  "terminal.integrated.cursorBlinking": true,
  "githubPullRequests.createOnPublishBranch": "never",
  "[javascript]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "redhat.telemetry.enabled": false,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "prettier.singleQuote": true,
  "editor.wordWrap": "on",
  "editor.linkedEditing": true,
  "bookmarks.useWorkaroundForFormatters": true,
  "cSpell.language": "en,en-GB,en-CA",
  "cSpell.enabled": false,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "zenMode.fullScreen": false,
  "zenMode.hideLineNumbers": false,
  "svelte.enable-ts-plugin": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.scrollback": 10000,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "javascript.format.semicolons": "insert",
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "git.confirmSync": false,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "typescript.format.semicolons": "insert",
  "typescript.tsserver.maxTsServerMemory": 5120,
  "files.maxMemoryForLargeFilesMB": 5120,
  "eslint.lintTask.enable": true,
  "eslint.options": {
  },
  "prettier.configPath": "./.prettierrc.json",
  "svelte.plugin.svelte.format.config.singleQuote": true,
  "svelte.plugin.svelte.format.enable": false,
  "window.titleBarStyle": "custom",
  "window.menuBarVisibility": "compact",
  "githubPullRequests.pullBranch": "never",
  "discord.removeRemoteRepository": true,
  "discord.idleTimeout": 14400,
  "[elixir]": {
    "editor.defaultFormatter": "JakeBecker.elixir-ls"
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "elixirLS.projectDir": "./",
  "vitest.enable": true,
  "vitest.showFailMessages": true,
  "haskell.manageHLS": "GHCup",
  "haskell.ghcupExecutablePath": "~/.ghcup/bin/ghcup",
  "discord.detailsEditing": "Editing a {LANG} file",
  "discord.detailsDebugging": "Debugging a {LANG} file",
  "workbench.colorTheme": "Catppuccin Macchiato",
  "[python]": {
    "editor.formatOnType": true
  },
  "ruby.languageServer": {},
  "typescript.updateImportsOnFileMove.enabled": "never",
  "workbench.iconTheme": "icons",
  "editor.minimap.renderCharacters": false,
  "workbench.sideBar.location": "right",
  "svelte.plugin.css.diagnostics.enable": false,
  "errorLens.excludeBySource": [
    "ts(2559)",
    "ts(2307)"
  ],
  "svelte.plugin.svelte.note-new-transformation": false
}
